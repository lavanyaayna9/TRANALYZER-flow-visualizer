#!/usr/bin/env awk
#
# Output Bytes
#
# Dependencies:
#   - basicStats
#
# Example:
#   - tawk '{ print obyt() }' file

@include "hdr"
@include "_validate_col"

function obyt(        _c) {
    _c = _validate_col("l7BytesSnt;l4BytesSnt;l3BytesSnt;l2BytesSnt", _my_bytesSnt)
    if (hdr()) return "Out Byte"
    return $_c
}
