-- Top unknown HTTPS JA3S signatures by number of flows

SELECT
    ja3hash,
    COUNT(*) AS cnt

FROM (
    SELECT
        ja3hash
    FROM
        flow
    ARRAY JOIN
        "sslJA3Hash" AS ja3hash
    WHERE
        bitTest("flowStat", 0) = 1
        AND notEmpty("sslJA3Hash")
        AND empty("sslJA3Desc")
        AND "timeFirst" >= {time_from:DateTime64(6)}
        AND "timeLast" <= {time_to:DateTime64(6)}
    SETTINGS
        enable_unaligned_array_join = 1
) AS flows

GROUP BY ja3hash
ORDER BY multiply(cnt, {sort_order:Int8})

LIMIT {n:UInt64}
