# [!!! -------------------------------------------------------------------- !!!]
# [!!! ---------------------- DO NOT EDIT THIS FILE ----------------------- !!!]
# [!!! -------------------------------------------------------------------- !!!]
#
# This is the default configuration for utils
#
# Apply the configuration as follows:
#     - t2conf utils --reset
#
# If you want to customize the default configuration, proceed as follows:
#     1. Create 'utils.config':
#           t2conf utils -g
#     2. Edit 'utils.config'
#     3. Apply 'utils.config':
#           t2conf utils -C utils.config
#
# [Format of the file]
#     - Empty lines and lines starting with '%' or '#'â€™ are ignored.
#     - One section starting with '# :name: pluginName" per file.
#     - If no section starting with '# :file: path/to/file.h" is specified,
#       it is assumed the file is src/pluginName.h (relative to plugin folder)
#         - If a configuration element applies to a different file, a file
#           section MUST be used
#         - Filenames are relative to plugin folder, i.e., utils,
#           e.g., bin2txt.h
#     - A line is composed of the name of a macro (define) in the header file
#       followed by an equal sign and the value to set for the define:
#           NAME = value
#       where value can be anything, including yes (= 1) and no (= 0)
#     - Add a comment before each setting to describe the configuration option
#       and its valid values:
#           # Description of the option [yes / no]
#       or
#           # Description of the option [0 - 255]
#       or
#           # Description of the option:
#           #   0: description of value 0
#           #   1: description of value 1
#           #   2: description of value 2

# ---------------------------------------------------------------------------- #
# :name: utils                                                                 #
# ---------------------------------------------------------------------------- #

# ---------------------------------------------------------------------------- #
# :file: bin2txt.h                                                             #
# ---------------------------------------------------------------------------- #

# IPv4 addresses representation:
#   0: normal
#   1: normalized (padded with zeros)
#   2: hexadecimal
#   3: uint32
IP4_FORMAT = 0

# IPv6 addresses representation:
#    0: compressed
#    1: uncompressed
#    2: one 128-bits hex number
#    3: two 64-bits hex numbers
IP6_FORMAT = 0

# MAC addresses representation:
#   0: normal (edit MAC_SEP to change the separator)
#   1: one 64-bits hex number
#   2: one 64-bits number
MAC_FORMAT = 0

# Separator to use in MAC addresses: 11:22:33:44:55:66
MAC_SEP = ":"

# Representation of non-IPv4/IPv6 addresses in IP columns
B2T_NON_IP_STR = "-"

# Activates the UTF-8 sanitizer for strings [yes / no]
TFS_SAN_UTF8 = yes

# Print unix timestamps as human readable dates [yes / no]
B2T_TIMESTR = no

# Extended header in flow file [yes / no]
TFS_EXTENDED_HEADER = no

# Types in header file:
#   0: none
#   1: numbers
#   2: C types
TFS_NC_TYPE = 2

# Hexadecimal output format:
#   0: lower case
#   1: upper case
HEX_CAPITAL = 0

# Start characters to label comments
HDR_CHR = "%"

# Column separator in the flow file
# (the following characters should not be used: ; . _ and ")
SEP_CHR = "\t"

# Output empty fields in JSON [yes / no]
JSON_KEEP_EMPTY = no

# Add spaces to make the JSON output more readable [yes / no]
JSON_PRETTY = no

# ---------------------------------------------------------------------------- #
# :file: fsutils.h                                                             #
# ---------------------------------------------------------------------------- #

# Min process file descriptors limit required
FSUTILS_MIN_FD_REQUIRED = 200

# Number of file descriptors to keep away from the file manager
FSUTILS_SPARE_FD = 100

# Maximum number of file descriptors to keep open
FSUTILS_MAX_OPEN_FD = 20

# ---------------------------------------------------------------------------- #
# :file: gz2txt.h                                                             #
# ---------------------------------------------------------------------------- #

# Activate code for gzip-(de)compression [yes / no]
USE_ZLIB = yes

# ---------------------------------------------------------------------------- #
# :file: rbTree.h                                                              #
# ---------------------------------------------------------------------------- #

# Enable debug output [yes / no]
RBT_DEBUG = no

# Activate the Red-Black Tree feature of rotating an unbalanced tree [yes / no]
RBT_ROTATION = no

# ---------------------------------------------------------------------------- #
# :file: subnetHL.h                                                            #
# ---------------------------------------------------------------------------- #

# IP range definition
#    0: CIDR only
#    1: Begin-End
SUBRNG = 0

# Output the county and the city [yes / no]
CNTYCTY = no

# Add whois address info [yes / no]
WHOADDR = no

# Use mmap to load the subnet file [yes / no]
SUB_MAP = yes

# Length of County record [0 - 255]
CNTYLEN = 14

# Length of City record [0 - 255]
CTYLEN = 14

# Length of Organization record [0 - 255]
WHOLEN = 30

# Length of Address record [0 - 255]
ADDRLEN = 30

# Representation of unknown locations
SUBNET_UNK = "-"

# ---------------------------------------------------------------------------- #
# :file: t2log.h                                                               #
# ---------------------------------------------------------------------------- #

# Color log messages [yes / no]
T2_LOG_COLOR = yes
