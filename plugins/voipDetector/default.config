# [!!! -------------------------------------------------------------------- !!!]
# [!!! ---------------------- DO NOT EDIT THIS FILE ----------------------- !!!]
# [!!! -------------------------------------------------------------------- !!!]
#
# This is the default configuration for voipDetector
#
# Apply the configuration as follows:
#     - t2conf voipDetector --reset
#
# If you want to customize the default configuration, proceed as follows:
#     1. Create 'voipDetector.config':
#           t2conf voipDetector -g
#     2. Edit 'voipDetector.config'
#     3. Apply 'voipDetector.config':
#           t2conf voipDetector -C voipDetector.config
#
# [Format of the file]
#     - Empty lines and lines starting with '%' or '#'â€™ are ignored.
#     - One section starting with '# :name: pluginName" per file.
#     - If no section starting with '# :file: path/to/file.h" is specified,
#       it is assumed the file is src/pluginName.h (relative to plugin folder)
#         - If a configuration element applies to a different file, a file
#           section MUST be used
#         - Filenames are relative to plugin folder, i.e., voipDetector,
#           e.g., src/voipDetector.h
#     - A line is composed of the name of a macro (define) in the header file
#       followed by an equal sign and the value to set for the define:
#           NAME = value
#       where value can be anything, including yes (= 1) and no (= 0)
#     - Add a comment before each setting to describe the configuration option
#       and its valid values:
#           # Description of the option [yes / no]
#       or
#           # Description of the option [0 - 255]
#       or
#           # Description of the option:
#           #   0: description of value 0
#           #   1: description of value 1
#           #   2: description of value 2

# ---------------------------------------------------------------------------- #
# :name: voipDetector                                                          #
# ---------------------------------------------------------------------------- #

# Enable SIP decoder
#   0: Do not decode SIP
#   1: Enable SIP decoder
#   2: Enable SIP decoder and add RTP/SIP findex/ssrc flow correlation
VOIP_SIP = 2

# Enable RTP/SIP flow correlation enhancement
# (require VOIP_SIP = 2)
#   0: No
#   1: add RTP srcIP
#   2: add srcIP of SIP flow
VOIP_SIP_PRV = 0

# Enable RTP decoder [yes / no]
VOIP_RTP = yes

# Enable RTCP decoder [yes / no]
VOIP_RTCP = yes

# Save RTP content to VOIP_V_PATH [yes / no]
VOIP_SAVE = no

# Enable buffering of saved RTP content [yes / no]
# (require VOIP_SAVE = yes)
VOIP_BUFMODE = yes

# Restore back G.711 suppressed silences [yes / no]
# (require VOIP_SAVE = yes)
VOIP_SILREST = yes

# Empty VOIP_V_PATH before starting [yes / no]
# (prevent appending to existing files)
# (require VOIP_SAVE = yes)
VOIP_RMDIR = yes

# Offset for payload to be saved [0 - UINT16_MAX]
# (require VOIP_SAVE = yes)
VOIP_PLDOFF = 0

# Merge ops:
#   0: SSRC
#   1: findex
VOIP_SVFDX = 1

# Payload length check against snap payload length [yes / no]
VOIP_ANALEN = no

# Minimum packet length of a flow [0 - UINT32_MAX]
# (require VOIP_SAVE = yes)
VOIP_MINPKT = 1

# (require VOIP_SVFDX == 0)
# (VOIP_MINPKT = yes)
RTPFMAX = 20

# Maximal SIP caller name length [0 - 255]
SIPNMMAX = 35

# Maximal SIP IP addr, m=audio, video ports [0 - UINT16_MAX]
SIPRFXMAX = 100

# Size of the per flow RTP buffer [0 - UINT32_MAX]
# (require VOIP_SAVE = yes)
RTPBUFSIZE = 4096

# Maximal SIP state requests [0 - 255]
SIPSTATMAX = 8

# Maximal number of version violations [0 - UINT16_MAX]
RTPMAXVERS = 1

# Maximal SIP state requests name length [0 - 255]
SIPCLMAX = 3

# Path for extracted content
# (require VOIP_SAVE = yes)
VOIP_V_PATH = "/tmp/TranVoIP"

# Default content file name prefix
# (require VOIP_SAVE = yes)
VOIP_FNAME = "nudel"

# File permissions
# (require VOIP_SAVE = yes)
VOIP_PERM = S_IRWXU
