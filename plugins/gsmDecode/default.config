# [!!! -------------------------------------------------------------------- !!!]
# [!!! ---------------------- DO NOT EDIT THIS FILE ----------------------- !!!]
# [!!! -------------------------------------------------------------------- !!!]
#
# This is the default configuration for gsmDecode
#
# Apply the configuration as follows:
#     - t2conf gsmDecode --reset
#
# If you want to customize the default configuration, proceed as follows:
#     1. Create 'gsmDecode.config':
#           t2conf gsmDecode -g
#     2. Edit 'gsmDecode.config'
#     3. Apply 'gsmDecode.config':
#           t2conf gsmDecode -C gsmDecode.config
#
# [Format of the file]
#     - Empty lines and lines starting with '%' or '#'â€™ are ignored.
#     - One section starting with '# :name: pluginName" per file.
#     - If no section starting with '# :file: path/to/file.h" is specified,
#       it is assumed the file is src/pluginName.h (relative to plugin folder)
#         - If a configuration element applies to a different file, a file
#           section MUST be used
#         - Filenames are relative to plugin folder, i.e., gsmDecode,
#           e.g., src/gsmDecode.h
#     - A line is composed of the name of a macro (define) in the header file
#       followed by an equal sign and the value to set for the define:
#           NAME = value
#       where value can be anything, including yes (= 1) and no (= 0)
#     - Add a comment before each setting to describe the configuration option
#       and its valid values:
#           # Description of the option [yes / no]
#       or
#           # Description of the option [0 - 255]
#       or
#           # Description of the option:
#           #   0: description of value 0
#           #   1: description of value 1
#           #   2: description of value 2

# ---------------------------------------------------------------------------- #
# :name: gsmDecode                                                             #
# ---------------------------------------------------------------------------- #

# Save ARFCN in a separate file [yes / no]
GSM_ARFCNFILE = yes

# Save calls in a separate file [yes / no]
GSM_CALLFILE = yes

# Save channels in a separate file [yes / no]
GSM_CDFILE = yes

# Save Immediate Assignments in a separate file [yes / no]
GSM_IMMASSFILE = yes

# Save IMSI/TMSI/IMEI/IMEISV in a separate file [yes / no]
GSM_IMSIFILE = yes

# Save operator names in a separate file [yes / no]
GSM_OPFILE = yes

# Save SMS in a separate file [yes / no]
GSM_SMSFILE = yes

# Create new files every GSM_ROTATE_TIME seconds [0 - UINT32_MAX]
# (use 0 to deactivate the feature)
# (require at least one of the above to be yes)
GSM_ROTATE_TIME = 0

# Save audio conversations [yes / no]
GSM_SPEECHFILE = yes

# Save GSM statistics in a separate file [yes / no]
GSM_STATFILE = yes

# Speech frames handling:
#   0: Save A and B flows in the same file
#   1: Create one file per direction
# (require GSM_SPEECHFILE == yes)
GSM_SPEECH_SPLIT = 1

# Format for TMSI:
#   0: Integer
#   1: Hexadecimal
GSM_TMSI_FORMAT = 1

# Folder for extracted audio conversations
# (require GSM_SPEECHFILE == yes)
GSM_SPEECH_DIR = "/tmp/gsm_speech"

# Folder for output files
GSM_TXT_DIR = "/tmp/gsm_txt"

# Empty GSM_SPEECH_DIR before starting
# (require GSM_SPEECHFILE == yes)
GSM_RMDIR = yes

# Suffix for ARFCN file
# (require GSM_ARFCNFILE == yes)
GSM_ARFCNFILE_SUFFIX = "_gsm_arfcn"

# Suffix for calls file
# (require GSM_CALLFILE == yes)
GSM_CALLFILE_SUFFIX = "_gsm_calls"

# Suffix for channels file
# (require GSM_CDFILE == yes)
GSM_CDFILE_SUFFIX = "_gsm_channels"

# Suffix for Immediate Assignments file
# (require GSM_IMMASSFILE == yes)
GSM_IMMASSFILE_SUFFIX = "_gsm_imm_ass"

# Suffix for IMSI file
# (require GSM_IMSIFILE == yes)
GSM_IMSIFILE_SUFFIX = "_gsm_imsi"

# Suffix for operators file
# (require GSM_OPFILE == yes)
GSM_OPFILE_SUFFIX = "_gsm_operators"

# Suffix for SMS file
# (require GSM_SMSFILE == yes)
GSM_SMSFILE_SUFFIX = "_gsm_sms"

# File extension for audio files
# (require GSM_SPEECHFILE == yes)
GSM_FILES_AMR_EXT = ".amr"

# File extension for temporary files
# (require one of GSM_*FILE == yes)
GSM_FILES_TMP_EXT = ".tmp"

# File extension for text files
# (require one of GSM_*FILE == yes)
GSM_FILES_TXT_EXT = ".txt"

# Suffix for GSM statistics file
# (require GSM_STATFILE == yes)
GSM_STATFILE_SUFFIX = "_gsm_stats.txt"

# Print generic debug messages [yes / no]
GSM_DEBUG = no

# Report unknown values for other messages [yes / no]
# (require GSM_DEBUG == yes)
GSM_DBG_UNK = no

